extends index.pug 
block content 
    ul 
        each schedule in schedules
            li 
                a(href='http://localhost:3000/schedules/'+schedule.ID_user) ID user: #{schedule.ID_user}
                p Day: #{schedule.day}
                p Start at: #{schedule.start_at} 
                p End at: #{schedule.end_at} 
    a(href='http://localhost:3000/schedules' class='addshift') Add your shift